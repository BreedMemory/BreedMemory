<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape
            android:shape="oval"
            android:useLevel="false">
            <solid
                android:color="#EFD487" />
            <padding
                android:bottom="15dp"
                android:left="15dp"
                android:right="15dp"
                android:top="15dp"/>
        </shape>
    </item>
    <item>
        <shape
            android:shape="oval"
            android:useLevel="false">
            <solid
                android:color="#EFD487" />
            <stroke android:color="@color/white"
                android:dashGap="5dip"
                android:dashWidth="2dip"
                android:width="1dip"/>
        </shape>
    </item>
</layer-list>